<Window x:Class="NoviHotelWPF.UnosParametaraPosetaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Unos datuma za izvestaj" Height="300" Width="300">
    <Grid>
        <Label Content="Unesite datum za generisanje izvestaja" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" />
        <DatePicker Height="25" Name="datePickerPocetni" Width="120" DisplayDate="12/12/2015" Language="sr" Margin="140,60,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <DatePicker Height="25" Name="datePickerZavrsni" Width="120" DisplayDate="1/1/2016" Language="sr" Margin="140,120,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Label Content="Datum pocetka" HorizontalAlignment="Left" Margin="35,60,0,0" VerticalAlignment="Top"/>
        <Label Content="Datum zavrsetka" HorizontalAlignment="Left" Margin="35,120,0,0" VerticalAlignment="Top"/>
        <Button Name="btnPrikazi" Content="Prikazi izvestaje o posetama hotela" HorizontalAlignment="Left" Margin="35,211,0,0" VerticalAlignment="Top" Width="225" Height="34" Click="btnPrikazi_Click"/>

    </Grid>
</Window>
